--
-- Table structure for table `projectwppnv_queue`
--

DROP TABLE IF EXISTS `projectwppnv_queue`;
/*!40101 SET @saved_cs_client     = @@character_set_client */;
/*!40101 SET character_set_client = utf8 */;
CREATE TABLE `projectwppnv_queue` (
  `id` bigint(20) NOT NULL AUTO_INCREMENT,
  `job` text COLLATE utf8mb4_unicode_ci NOT NULL,
  `attempts` tinyint(1) NOT NULL DEFAULT 0,
  `locked` tinyint(1) NOT NULL DEFAULT 0,
  `locked_at` datetime DEFAULT NULL,
  `available_at` datetime NOT NULL,
  `created_at` datetime NOT NULL,
  PRIMARY KEY (`id`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_unicode_ci;
/*!40101 SET character_set_client = @saved_cs_client */;

--
-- Dumping data for table `projectwppnv_queue`
--

LOCK TABLES `projectwppnv_queue` WRITE;
/*!40000 ALTER TABLE `projectwppnv_queue` DISABLE KEYS */;
/*!40000 ALTER TABLE `projectwppnv_queue` ENABLE KEYS */;
UNLOCK TABLES;

--
-- Table structure for table `projectwppnv_term_relationships`
